{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default-capabilities",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-execute",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "/bin/zsh",
          "cmd": "/bin/zsh",
          "args": true
        }
      ]
    }
  ]
}